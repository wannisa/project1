<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.aoyler.pawarisaclinicapp">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.USE_CREDENTIALS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/two"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="menu"></activity>
        <activity android:name="com.example.aoyler.pawarisaclinicapp.MenuHome"></activity>
        <activity android:name="com.example.aoyler.pawarisaclinicapp.SelectDate"></activity>
        <activity android:name="com.example.aoyler.pawarisaclinicapp.Notifications"></activity>
        <activity android:name="com.example.aoyler.pawarisaclinicapp.CalendarSelect"></activity>
        <activity android:name="com.example.aoyler.pawarisaclinicapp.CalendarSelecter"></activity>
        <!-- ATTENTION: This was auto-generated to add Google Play services to your project for
             App Indexing.  See https://g.co/AppIndexing/AndroidStudio for more information. -->
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
    </application>

</manifest>